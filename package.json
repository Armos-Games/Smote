{
  "name": "smote",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Armos-Games/Smote"
  },
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.6.0"
  },
  "scripts": {
